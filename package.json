{
  "name": "battle-maps",
  "version": "0.16.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:lan": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build && cp dist/battle-maps/index.html dist/battle-maps/200.html",
    "build:prod": "ng build --prod && cp dist/battle-maps/index.html dist/battle-maps/200.html",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:cvg": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --webdriverUpdate=false",
    "commit": "cz",
    "checks": "npm run test:ci && npm run e2e && npm run lint"
  },
  "private": true,
  "dependencies": {
